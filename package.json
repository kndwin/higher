  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate": "npx prisma generate && npx graphql-codegen --config graphqlcodegen.yml",
    "generate:client": "graphql-codegen --config graphqlcodegen.yml",
    "generate:server": "prisma generate",
    "husky:prepare": "husky install",
